---
import "../styles/global.css";

interface Props {
    title: string;
}



const {title} = Astro.props;
---



<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="imagen/svg+xml" href="/public/Logo-head.jpg" />
    <meta name="generator" content={Astro.generator}>

    <title>{title}</title>

</head>
<body class="bg-slate-100 text-stone-950">
    <main>
        <slot />
    </main>
</body>
</html>